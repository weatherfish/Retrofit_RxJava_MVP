<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SwipeBackLayout">
        <attr name="edge_size" format="dimension" />
        <attr name="edge_flag">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="bottom" value="2" />
            <enum name="all" value="3" />
        </attr>
        <attr name="shadow_left" format="reference" />
        <attr name="shadow_right" format="reference" />
        <attr name="shadow_bottom" format="reference" />
    </declare-styleable>

    <attr name="textColor" format="reference|color" />
    <attr name="titleColor" format="reference|color" />
    <attr name="backgroundColor" format="reference|color" />
    <attr name="toolbarTextColor" format="reference|color" />
    <attr name="toolbarbackgroundColor" format="reference|color" />
    <attr name="tab_TextColor" format="reference|color" />
    <attr name="tab_BackgroundColor" format="reference|color" />
    <attr name="tab_SelectedTextColor" format="reference|color" />
    <attr name="tab_IndicatroColor" format="reference|color" />
    <attr name="headViewColor" format="reference|color" />

    <attr name="SwipeBackLayoutStyle" format="reference" />
</resources>